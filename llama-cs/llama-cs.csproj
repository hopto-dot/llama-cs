<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <PackageId>hopto-dot.llama_cs</PackageId>
    <RootNamespace>llama_cs</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<Authors>hopto-dot</Authors>
	<PackageProjectUrl>https://github.com/hopto-dot/llama-cs</PackageProjectUrl>
	<RepositoryUrl>https://github.com/hopto-dot/llama-cs</RepositoryUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<Version>0.0.1.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	<None Include="README.md" Pack="true" PackagePath=""/>
  </ItemGroup>

</Project>
